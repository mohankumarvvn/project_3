<!DOCTYPE html>
<html lang="">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width,height=device-height, initial-scale=1">
		<title>Welcome to mysite</title>

		<!-- Bootstrap CSS -->
		<link rel="stylesheet" href="css/normalize/normalize.css">
		<link href="css/bootstrap.css" rel="stylesheet">
		<link href="my_project.css" rel="stylesheet">
		<!--[if IE 6]>
			<script src="js/html5shiv/dist/html5shiv.js"></script>
		<![endif]-->

		<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
		<!--[if lt IE 9]>
			<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
			<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
		<![endif]-->
		<script type="text/javascript" src="js/jquery.js"></script>
		<script type="text/javascript" src="js/bootstrap.js"></script>
		<script type="text/javascript" src="my_project.js"></script>
	</head>
	<body>
		<div class="body_content">
			<header class="content_header">
				<div class="row">
					<div class="col-lg-6">
						<div class="logo">
							<a href="#content_home"><img src="images/logo.png" alt="logo" class="logo go_home"></a>
						</div>
					</div>
					<div class="col-lg-6">
						<nav>
							<ul class="menu">
								<li><a href="#content_home"><img class="go_home" src="images/home.png" alt="Home"></a></li>
								<li><a href="#content_portfolio"><img class="go_portfolio" src="images/portfolio.png" alt="Portfolio"></a></li>
								<li><a href="#content_contact"><img class="go_contact" src="images/contact.png" alt="contact"></a></li>
							</ul>
						</nav>
							<img id="pointer" src="images/pointer.png" alt="pointer">
					</div>
				</div>
			</header>
			<div class="content_body">
				<div id="content_home" class="content_home">
					<div class="arrow">
						<a href="#content_portfolio"><img class="go_portfolio" src="images/home_arrow.png" alt=""></a>
						<div class="arrow_port"></div>
					</div>
				</div>
				<div id="content_portfolio" class="content_portfolio">
					<div class="port_header">
						<h1>Portfolio</h1>
					</div>
					<div class="parallax parallax_1">
						
					</div>
					<div class="parallax_content parallax_logo">
						
					</div>
					<div class="parallax parallax_2">
						
					</div>
					<div class="parallax_content parallax_desktop">
						
					</div>
					<div class="parallax parallax_3">
						
					</div>
					<div class="parallax_content parallax_mobile">
						
					</div>
					<div class="arrow">
						<a href="#content_contact"><img class="go_contact" src="images/portfolio_arrow.png" alt=""></a>
						<div class="arrow_cont"></div>
					</div>
				</div>
				<div id="content_contact" class="content_contact">
					<div class="row contact_row">
						<div class="col-lg-6">
							<input type="text" placeholder="Name" class="contact_input"/>
							<input type="text" placeholder="Name" class="contact_input"/>							
						</div>
						<div class="col-lg-6">
							<textarea name="" placeholder="ask your queries..." id="contact_text" cols="30" rows="4"></textarea>
						</div>
						<div class="col-lg-12 ">
							<div class="contact_send btn">SEND</div>							
						</div>
					</div>
				</div>
				<footer class="content_footer">
					<div class="arrow">
						<a href="#content_home"><img class="go_home" src="images/contact_arrow.png" alt=""></a>
						<div class="arrow_home"></div>						
					</div>
				</footer>
			</div>
		</div>
	</body>
</html>